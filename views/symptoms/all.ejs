<div class="text-center">
	<h1 class="jumbotron">Which of these do you have?</h1>
	<img src="http://img.thebody.com/thebody/2016/symptoms_600px.jpg" alt="">
	<% symptoms.forEach(function(symptom){ %>
			<div class="well">
				<a href="/symptoms/<%=symptom.id%>">
					<%=symptom.name%>
				</a>
			</div>
	<% }); %>
</div>
